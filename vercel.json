{
    "version": 2,
    "builds": [
      {
        "src": "src/**/*.{js,ts,jsx,tsx}",
        "use": "@vercel/node"
      },
      {
        "src": "api/**/*.ts",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/server.ts"
      }
    ]
  }
  