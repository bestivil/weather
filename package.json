{
  "name": "react-typescript-boilerplate",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.15.2",
    "@mui/material": "^5.15.2",
    "@mui/system": "^5.15.2",
    "@stylexjs/stylex": "^0.3.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.7.13",
    "@types/react": "^17.0.20",
    "@types/react-dom": "^17.0.9",
    "autoprefixer": "^10.4.16",
    "concurrently": "^7.0.0",
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "node-fetch": "^3.3.2",
    "nodemon": "^3.1.0",
    "postcss-cli": "^11.0.0",
    "react": "^17.0.2",
    "react-burger-menu": "^3.0.9",
    "react-dom": "^17.0.2",
    "react-scripts": "^5.0.1",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^4.4.2",
    "web-vitals": "^2.1.0"
  },
  "scripts": {
    "start": "react-scripts start && node server/server.ts",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "prettier": "prettier . --ignore-path .gitignore",
    "format:check": "prettier . --ignore-path .gitignore --check",
    "format:fix": "prettier . --ignore-path .gitignore --write",
    "lint": "eslint --ext .tsx,.ts .",
    "validate": "concurrently \"yarn run lint\" \"yarn run type:check\" \"yarn run format:check\"",
    "type:check": "tsc",
    "prepare": "husky install",
    "server": "node dist/server.js",
    "build:server": "tsc -p tsconfig.server.json",
    "dev:server": "nodemon"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/node-fetch": "^2.6.11",
    "@types/react-burger-menu": "^2.8.7",
    "@typescript-eslint/eslint-plugin": "^5.9.0",
    "@typescript-eslint/parser": "^5.9.0",
    "eslint": "^8.56.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-security": "^1.4.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.5",
    "prettier": "^2.5.1"
  },
  "lint-staged": {
    "*.{tsx,ts}": "eslint --fix",
    "*.{js,css,md}": "prettier --write"
  }
}
